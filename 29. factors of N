#include <stdio.h>

void printFactors(int n) {
    printf("Factors of %d are: ", n);
    for (int i = 1; i <= n; i++) {
        if (n % i == 0)
            printf("%d ", i);
    }
    printf("\n");
}

int main() {
    int n = 28;
    printf("A.Harshavardhan K.R\n");
printf("192210042\n"); 
    printFactors(n);
    return 0;
}
