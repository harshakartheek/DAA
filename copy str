#include <stdio.h>
#include <string.h>

int main() 
{
 	printf("A.Harshavardhan K.R\n");
	printf("192210042\n");
    char str1[100];
    char str2[100];
    printf("Enter a string: ");
    scanf("%s", str1);
    int i;
    for (i = 0; str1[i] != '\0'; i++) 
	{
        str2[i] = str1[i];
    }
    str2[i] = '\0'; 
    printf("Copied string: %s\n", str2);
    return 0;
}
